{
  "name": "aragon-token-manager",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@aragon/client": "^1.0.0-beta.1",
    "@aragon/ui": "^0.5.0",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "styled-components": "^3.1.6"
  },
  "devDependencies": {
    "react-scripts": "1.0.17"
  },
  "scripts": {
    "sync-assets": "rsync -rtu \"$(dirname $(node -p 'require.resolve(\"@aragon/ui\")'))/\" public/aragon-ui",
    "start": "npm run sync-assets && PORT=3003 react-scripts start",
    "build": "npm run sync-assets && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "proxy": "http://localhost:3003"
}
