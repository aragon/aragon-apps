{
  "roles": [
    {
      "name": "Transfer Agent's tokens",
      "id": "TRANSFER_ROLE",
      "params": [
        "Token address",
        "Receiver address",
        "Token amount"
      ]
    },
    {
      "name": "Execute actions",
      "id": "EXECUTE_ROLE",
      "params": [
        "Target address",
        "ETH value",
        "Signature"
      ]
