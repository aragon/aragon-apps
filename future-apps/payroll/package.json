    "babel-helpers": "^6.24.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-2": "^6.24.1",
    "babel-preset-stage-3": "^6.17.0",
    "babel-register": "^6.26.0",
    "ganache-cli": "^6.1.0",
    "solidity-coverage": "0.3.5",
