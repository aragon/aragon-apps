{
  "name": "@aragon/future-apps-payroll",
  "version": "0.1.0",
  "scripts": {
    "start": "aragon run --network rpc --kit PayrollKit --kit-init @ARAGON_ENS",
    "dev": "aragon run --network rpc --http localhost:8001 --http-served-from ./dist",
    "dev:reset": "npm run dev -- --reset",
    "test": "TRUFFLE_TEST=true npm run ganache-cli:test",
    "test:gas": "GAS_REPORTER=true npm test",
    "lint": "solium --dir ./contracts",
    "coverage": "SOLIDITY_COVERAGE=true npm run ganache-cli:test",
    "ganache-cli:test": "./node_modules/@aragon/os/scripts/ganache-cli.sh",
    "start:ui": "npm run sync-assets && npm run build:script -- --no-minify && parcel serve app/index.html --port 8001 --open",
    "build": "npm run sync-assets && npm run build:app && npm run build:script",
    "build:app": "parcel build app/index.html --public-url '.' --no-cache",
    "build:script": "parcel build app/script.js --no-cache",
    "lint:app": "standard app/**/*.js",
    "sync-assets": "copy-aragon-ui-assets -n aragon-ui ./dist"
  },
