{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "browsers": [
            "> 1%",
            "last 3 versions",
            "ie >= 9",
            "ios >= 8",
            "android >= 4.2"
          ]
        },
        "useBuiltIns": 'entry',
        "core-js": 3
      }
    ],
    "@babel/preset-react"
  ],
  "plugins": [
    ["styled-components", { "displayName": true }],
    "@babel/plugin-proposal-class-properties"
  ]
}
