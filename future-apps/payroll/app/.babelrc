{
  "presets": [
    [
      "@babel/preset-env",
      {
        "modules": false,
        "targets": {
          "browsers": [
            "> 2%",
            "last 3 versions",
            "last 1 edge versions",
            "ie >= 9",
            "ios >= 8",
            "android >= 4.2",
            "not op_mini all"
          ]
        }
      }
    ]
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-transform-runtime",
    "babel-plugin-styled-components"
  ]
}
