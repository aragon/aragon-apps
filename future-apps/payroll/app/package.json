{
  "name": "aragon-payroll",
  "version": "0.0.1",
  "private": true,
  "author": "Sebasti√°n Galiano <sebastian.galiano@altoros.com>",
  "scripts": {
    "build": "npm run sync-assets && npm run build:app && npm run build:script",
    "build:app": "parcel build src/index.html --public-url '.' --no-cache",
    "build:script": "parcel build src/script.js --no-cache",
    "lint": "standard --parser babel-eslint src/**/*.js",
    "start": "npm run sync-assets && npm run build:script -- --no-minify && parcel serve src/index.html --port 8001 --open",
    "sync-assets": "copy-aragon-ui-assets -n aragon-ui ./dist",
    "test": "jest",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "@aragon/client": "^1.1.0",
    "@aragon/ui": "^0.27.0",
    "@babel/runtime": "^7.1.2",
    "ajv": "^6.5.4",
    "ajv-keywords": "^3.2.0",
    "bn.js": "^4.11.8",
    "date-fns": "2.0.0-alpha.22",
    "prop-types": "^15.6.2",
    "react": "^16.6.0",
    "react-autocomplete": "^1.8.1",
    "react-dom": "^16.6.0",
    "styled-components": "^3.4.10",
    "web3-utils": "^1.0.0-beta.36"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-plugin-react-remove-properties": "^0.2.5",
    "babel-plugin-styled-components": "^1.8.0",
    "jest": "^23.6.0",
    "jest-dom": "^2.1.0",
    "parcel-bundler": "^1.10.3",
    "react-testing-library": "^5.2.3",
    "regenerator-runtime": "^0.12.1",
    "standard": "*"
  }
}
